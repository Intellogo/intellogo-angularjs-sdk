{
    "browser": true,
    "bitwise": true,
    "camelcase": true,
    "curly": true,
    "esnext": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": "nofunc",
    "maxcomplexity": 5,
    "maxdepth": 3,
    "maxlen": 100,
    "maxparams": 4,
    "maxstatements": 15,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "quotmark": "single",
    "smarttabs": true,
    "strict": true,
    "trailing": true,
    "undef": true,
    "unused": true,
    "globals": {
        "$": false,
        "_": false,
        "alert": false,
        "angular": false,
        "async": false,
        "SparkMD5": false,
        "moment": false
    }
}
